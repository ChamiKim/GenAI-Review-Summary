# 동시 판매 상품 찾기

WITH PRODUCTS AS (
    SELECT PRD_CD
    FROM CDP.SF_CDPDW.D_CHN_PRD_MSTR
    WHERE (CHN_CD = '036')
        OR (CHN_CD = '031' AND CHN_BRND_NM = 'INNISFREE')
        AND (PRD_CD != 'Z')
    GROUP BY PRD_CD
    HAVING COUNT(DISTINCT CHN_CD) = 2)
SELECT *
FROM CDP.SF_CDPDW.D_CHN_PRD_MSTR
WHERE PRD_CD IN (SELECT PRD_CD FROM PRODUCTS)
    AND (CHN_CD = '036')
        OR (CHN_CD = '031' AND CHN_BRND_NM = 'INNISFREE')
    AND (PRD_CD != 'Z')
