SELECT *
FROM CDP.SF_CDPDW.F_LOG_GA_HIST
WHERE STND_YMD BETWEEN '2024-01-01' AND '2024-03-31'
    AND SESS_ID IN (SELECT SESS_ID
                    FROM CDP.SF_CDPDW.F_LOG_GA_HIST
                    WHERE STLM_STP = 4
                        AND SITE_ID IN ('UA-110770460-3', 'UA-110770460-1'))
    AND SITE_ID IN ('UA-110770460-3', 'UA-110770460-1') -- 이니스프리, 아모레몰
LIMIT 2000 ;
